#x-perspective-example {
	.normal,
	.perspective {
		width: 90%;
		margin: 5px auto;
		border: 1px solid #ccc;
	}
	.normal {
		<%=include%>x-perspective();	
	}
	.perspective {
		<%=include%>x-perspective(100px);
	}
	.child {
		<%=include%>x-inline-block();
		width: 30px;
		height: 30px;
		margin: 5px;
		background: #ccc;
		<%=include%>x-transform(rotateY(30deg));
	}
}
