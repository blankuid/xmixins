#x-perspective-origin-example {
	.default,
	.origin {
		width: 90%;
		margin: 5px auto;
		border: 1px solid #ccc;
		<%=include%>x-perspective(100px);
	}
	.default {
		<%=include%>x-perspective-origin();	
	}
	.origin {
		<%=include%>x-perspective-origin(top left);
	}
	.child {
		<%=include%>x-inline-block();
		width: 30px;
		height: 30px;
		margin: 5px;
		background: #ccc;
		<%=include%>x-transform(rotateY(30deg));
	}
}

